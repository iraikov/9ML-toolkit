   |  state_root (D.ContState (x, y, ext, extev, ynext, yrsp)) = false

  fun copy_array a =
    let val len = Array.length a
        val b = if len>0 then Array.array (len, Array.sub(a,0)) else a
    in
        if len>0 then Array.copy {di=0, dst=b, src=a} else (); b
    end
                                                                     
  fun copy_state (D.RegimeState (x, y, xev, ev, d, r, ext, extev, ynext, yrsp, enext, root)) =
    D.RegimeState(x, copy_array y, xev, copy_array ev, copy_array d, copy_array r,
                  copy_array ext, copy_array extev, ynext, yrsp, enext, root)
   |  copy_state (D.EventState (x, y, xev, ev, ext, extev, ynext, yrsp, enext, root)) =
      D.EventState (x, copy_array y, xev, copy_array ev, copy_array ext, copy_array extev,
                    ynext, yrsp, enext, root) 
   |  copy_state (D.ContState (x, y, ext, extev, ynext, yrsp)) =
      D.ContState (x, copy_array y, copy_array ext, copy_array extev, ynext, yrsp)
                                               

        
