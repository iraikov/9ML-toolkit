  fun {{pop.name}}_run f (statesample,extsample,evsample) (Wmap,n0) ((i,fld,input),(nstates,spikes,statelog)) =
    let 

        val f' = f (i,{{pop.name}}_parameters,fld)

(*
{% if pop.value.prototype.fieldExprML %}
        val fieldV = getindex ({{pop.name}}_field_vector,i)
{% endif %}
*)

        val update = Unsafe.Array.update
        val ext_i = state_ext input
        val extev_i = state_extev input

        val _ = 
            List.foldl
                (fn (port, index) => 
                    case IntMap.find (Wmap, port) of
                        NONE => 
                        (update (ext_i, index, 0.0); 
                         update (extev_i, index, Real.posInf); 
                         index+1)
                      | SOME (t,W) => 
                        case IntMap.find(W,i+n0) of
                            SOME v => 
                            (update (ext_i, index, v); 
                             update (extev_i, index, t); 
                             index+1)
                          | NONE => 
                            (update (ext_i, index, 0.0); 
                             update (extev_i, index, Real.posInf); 
                             index+1))
                0
                {{pop.name}}_ext_ports


        fun recur (nstate,spikes,statelog: (int * D.model_state) list) = 
            let
                val spikes' = if (Real.>= (event_sub(nstate,0), 0.0))
                              then ((i+{{pop.name}}_n0,1.0))::spikes
                              else spikes
                val statelog' = if (i<statesample) orelse (i<extsample) orelse (i<evsample) 
                                then ((i+{{pop.name}}_n0,copy_state nstate))::statelog
                                else statelog
            in
                if state_root nstate 
                then recur (f' nstate, spikes', statelog')
                else (nstate, spikes', statelog')
            end

        val (nstate,spikes',statelog') = recur (f' input,spikes,statelog)
    in 
        ((i,fld,nstate)::nstates, spikes', statelog')
         
    end

