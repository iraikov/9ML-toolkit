  fun make_initial_vector (alloc,bool_alloc,n,nev,ndsc,nregime) 
                          (i,condfun,initial,SOME evinitial,dinitial,SOME rinitial,extinitial,extevinitial) =
      let 
          val dinitial': real array -> real array = 
              case dinitial of
                  SOME v => v
                | NONE => (fn v => v)
      in
          D.condApply condfun (D.RegimeState (0.0, initial(alloc n), 0.0, evinitial (alloc nev), dinitial' (alloc ndsc), 
                                              rinitial (bool_alloc nregime), extinitial(), extevinitial(), alloc n, alloc n,
                                              alloc nev, false))
      end
   |  make_initial_vector (alloc,bool_alloc,n,nev,ndsc,nregime) 
                          (i,condfun,initial,SOME evinitial,NONE,NONE,extinitial,extevinitial) =
      D.condApply condfun (D.EventState (0.0, initial(alloc n), 0.0, evinitial (alloc nev), 
                                         extinitial (), extevinitial (), initial(alloc n), initial(alloc n), 
                                         alloc nev, false))
   |  make_initial_vector (alloc,bool_alloc,n,nev,ndsc,nregime) 
                          (i,condfun,initial,NONE,NONE,NONE,extinitial,extevinitial) =
      D.ContState (0.0, initial(alloc n), extinitial(), extevinitial(), alloc n, alloc n)
   |  make_initial_vector (alloc,bool_alloc,n,nev,ndsc,nregime) 
                          (i,condfun,initial,_,_,_,extinitial,extevinitial) =
      (putStrLn TextIO.stdOut "make_initial_vector:"; raise Domain)

